From 6a59588a073c2047bcc621743d699b864561f4aa Mon Sep 17 00:00:00 2001
From: abcdjdj <abcdjdj@gmail.com>
Date: Mon, 5 May 2014 16:01:46 +0530
Subject: [PATCH] LINARO

Change-Id: Id6c3778de8f00608a04157768af3d399bded8451
---
 core/combo/TARGET_linux-arm.mk | 1 +
 core/tasks/kernel.mk           | 4 ++--
 envsetup.sh                    | 2 +-
 3 files changed, 4 insertions(+), 3 deletions(-)

diff --git a/core/combo/TARGET_linux-arm.mk b/core/combo/TARGET_linux-arm.mk
index ece5b98..c664588 100644
--- a/core/combo/TARGET_linux-arm.mk
+++ b/core/combo/TARGET_linux-arm.mk
@@ -48,6 +48,7 @@ endif
 include $(TARGET_ARCH_SPECIFIC_MAKEFILE)
 
 # You can set TARGET_TOOLS_PREFIX to get gcc from somewhere else
+TARGET_TOOLS_PREFIX := prebuilts/gcc/$(HOST_PREBUILT_TAG)/arm/LINARO-master/bin/arm-linux-androideabi-
 ifeq ($(strip $(TARGET_TOOLS_PREFIX)),)
 TARGET_TOOLCHAIN_ROOT := prebuilts/gcc/$(HOST_PREBUILT_TAG)/arm/arm-linux-androideabi-$(TARGET_GCC_VERSION)
 TARGET_TOOLS_PREFIX := $(TARGET_TOOLCHAIN_ROOT)/bin/arm-linux-androideabi-
diff --git a/core/tasks/kernel.mk b/core/tasks/kernel.mk
index d27b6ba..4d8e2f4 100644
--- a/core/tasks/kernel.mk
+++ b/core/tasks/kernel.mk
@@ -147,10 +147,10 @@ ifeq ($(TARGET_ARCH),arm)
         ifeq ($(HOST_OS),darwin)
             ARM_CROSS_COMPILE:=CROSS_COMPILE="$(ccache) $(ANDROID_BUILD_TOP)/prebuilts/gcc/darwin-x86/arm/$(TARGET_KERNEL_CUSTOM_TOOLCHAIN)/bin/arm-eabi-"
         else
-            ARM_CROSS_COMPILE:=CROSS_COMPILE="$(ccache) $(ANDROID_BUILD_TOP)/prebuilts/gcc/linux-x86/arm/$(TARGET_KERNEL_CUSTOM_TOOLCHAIN)/bin/arm-eabi-"
+            ARM_CROSS_COMPILE:=CROSS_COMPILE="$(ccache) $(ANDROID_BUILD_TOP)/prebuilts/gcc/linux-x86/arm/LINARO-KERNEL/bin/arm-eabi-"
         endif
     else
-        ARM_CROSS_COMPILE:=CROSS_COMPILE="$(ccache) $(ARM_EABI_TOOLCHAIN)/arm-eabi-"
+        ARM_CROSS_COMPILE:=CROSS_COMPILE="$(ccache) $(ANDROID_BUILD_TOP)/prebuilts/gcc/linux-x86/arm/LINARO-KERNEL/bin/arm-eabi-"
     endif
     ccache = 
 endif
diff --git a/envsetup.sh b/envsetup.sh
index 25cccb2..dfbb82c 100644
--- a/envsetup.sh
+++ b/envsetup.sh
@@ -137,7 +137,7 @@ function setpaths()
     case $ARCH in
         x86) toolchaindir=x86/i686-linux-android-$targetgccversion/bin
             ;;
-        arm) toolchaindir=arm/arm-linux-androideabi-$targetgccversion/bin
+        arm) toolchaindir=arm/LINARO-master/bin
             ;;
         mips) toolchaindir=mips/mipsel-linux-android-$targetgccversion/bin
             ;;
-- 
